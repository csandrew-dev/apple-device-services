{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "PagingInformation",
  "description": "Paging information for data responses.",
  "x-apple-developer-api-uri": "https://developer.apple.com/documentation/applebusinessmanagerapi/paginginformation",
  "x-apple-developer-api-version": "1.1+",
  "type": "object",
  "required": [ "paging" ],
  "properties": {
    "paging": {
      "description": "The paging information details.",
      "$ref": "#/$defs/PagingInformation.Paging"
    }
  },
  "$defs": {
    "PagingInformation.Paging": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "title": "PagingInformation.Paging",
      "description": "Paging details, such as the total number of resources and the per-page limit.",
      "x-apple-developer-api-uri": "https://developer.apple.com/documentation/applebusinessmanagerapi/paginginformation/paging-data.dictionary",
      "x-apple-developer-api-version": "1.1+",
      "type": "object",
      "required": [ "limit" ],
      "properties": {
        "limit": {
          "description": "The maximum number of resources to return per page.",
          "type": "integer"
        },
        "nextCursor": {
          "description": "The cursor to use for the next request, in case of pagination.",
          "type": "string"
        },
        "total": {
          "description": "The total number of resources to return.",
          "type": "integer"
        }
      }
    }
  }
}
